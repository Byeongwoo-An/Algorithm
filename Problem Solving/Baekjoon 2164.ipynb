   "source": [
    "from collections import deque\n",
    "\n",
    "n = int(input())\n",
    "q = deque()\n",
    "\n",
    "for i in range(1,n+1):\n",
    "    q.append(i)\n",
    "\n",
    "while len(q) != 1:\n",
    "    q.popleft()\n",
    "    tmp = q.popleft()\n",
    "    q.append(tmp)\n",
    "\n",
    "print(q[0])"
   ]
  }
